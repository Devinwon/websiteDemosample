@{
    Layout = "~/Views/Shared/_StudentLayout.cshtml";
    ViewBag.Title = "缴费上报";
    var ParentActionName = ViewContext.RouteData.Values["Action"].ToString().ToLower();
    var ParentControllerName = ViewContext.RouteData.Values["controller"].ToString().ToLower();
}

<!-- START PAGE CONTAINER -->
<div class="page-container">

    @Html.Action("SideBar", "Common", new { ParentAction = ParentActionName, ParentController = ParentControllerName })

    <!-- PAGE CONTENT -->
    <div class="page-content">

        @Html.Action("BodyHeader", "Common")
        <!-- START BREADCRUMB -->
        <ul class="breadcrumb">
            <li><a href="#">云课堂</a></li>
            <li class="active">费用上报</li>
        </ul>
        <!-- END BREADCRUMB -->
        <!-- PAGE CONTENT WRAPPER -->
        <div class="page-content-wrap">
            <form class="form-inline" role="form">

                <div class="row">
                    <div class="col-md-12">
                        <div class="panel panel-default">
                            <div class="panel-body">
                                <div class="form-group">
                                    <label>选择学年：</label>
                                    <select id="selYear" name="selYear" class="form-control">
                                        <option value="0">== 选择学年 ==</option>
                                        <option value="1">第一学年</option>
                                        <option value="2">第二学年</option>
                                        <option value="3">第三学年</option>
                                    </select>

                                </div>
                                <button type="button" id="btnSelYear" class="btn btn-primary">选择</button>
                                <button type="button" id="btnAddFee" class="btn btn-danger pull-right">单据上传</button>
                            </div>
                        </div>
                    </div>

                </div>
            </form>

            <div class="row">
                <div class="col-md-12">
                    <div class="panel panel-default">
                        <div class="panel-body">

                            <!-- table content -->
                            <div id="fee-list" class="table-responsive">
                                <table class="table datatable_fee table-bordered table-actions table-hover">
                                    <thead>
                                        <tr>
                                            <th width="100" class="text-center">学年</th>
                                            <th width="200" class="text-center">单据上传日期</th>
                                            <th width="200">缴费科目</th>
                                            <th width="100" class="text-center">缴费金额</th>
                                            <th width="100" class="text-center">审核状态</th>
                                            <th>操作</th>
                                        </tr>
                                    </thead>
                                    <tbody id="QA_SelectList">
                                        <tr>
                                            <td valign="top" colspan="6" class="dataTables_empty">无数据</td>
                                        </tr>
                                        @*<tr>
                                            <td class="text-center">2013</td>
                                            <td class ="text-center">2015-08-06 12:12:12</td>
                                            <td>书本费</td>
                                            <td class="text-center"><span class="text-danger">800.00</span></td>
                                            <td class="text-center"><span class="label label-warning">待审核</span></td>
                                            <td>
                                                <button data-action="show" class="btn btn-primary btn-sm" data-id="15"><span class="fa fa-search"></span>查看</button>

                                            </td>
                                        </tr>
                                        <tr>
                                            <td class="text-center">2013</td>
                                            <td class="text-center">2015-08-06 12:12:12</td>
                                            <td>学杂费</td>
                                            <td class="text-center"><span class="text-danger">500.00</span></td>
                                            <td class="text-center"><span class="label label-success">已审核</span></td>
                                            <td>
                                                <button data-action="show" class="btn btn-primary btn-sm" data-id="15"><span class="fa fa-search"></span>查看</button>

                                            </td>
                                        </tr>
                                        <tr>
                                            <td class="text-center">2013</td>
                                            <td class="text-center">2015-08-06 12:12:12</td>
                                            <td>住宿费</td>
                                            <td class="text-center"><span class="text-danger">800.00</span></td>
                                            <td class="text-center"><span class="label label-danger">已退回</span></td>
                                            <td>
                                                <button data-action="show" class="btn btn-primary btn-sm" data-id="15"><span class="fa fa-search"></span>查看</button>

                                            </td>
                                        </tr>*@

                                    </tbody>
                                </table>
                            </div>
                            <!-- END table content -->
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <!-- END PAGE CONTENT WRAPPER -->
    </div>
    <!-- END PAGE CONTENT -->

</div>
<!-- END PAGE CONTAINER -->
<!-- Modal AJAX -->
<div id="pop_modaldialog">
</div>
<!-- END Modal AJAX -->
