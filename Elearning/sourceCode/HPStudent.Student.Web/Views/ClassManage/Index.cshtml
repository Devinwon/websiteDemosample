                                    <label>选择班级：</label>
                                    <select id="sel_Class" name="selClass" class="form-control">
                                        <option value="0">== 请选择 ==</option>
                                    </select>

                                </div>*@


                                <button type="button" id="btnSearchClass" class="btn btn-primary">查询</button>
                                <button id="btnAddClass" class="btn btn-primary btn-sm pull-right" type="button">
                                    <span class="fa fa-plus-square"></span> 添加实训班级
                                </button>
                            </div>
                        </div>
                    </div>

                </div>
            </form> 



                      

