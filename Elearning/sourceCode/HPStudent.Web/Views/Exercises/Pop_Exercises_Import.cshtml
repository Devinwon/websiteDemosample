<div class="modal" id="import-exercises" tabindex="-1" role="dialog" aria-labelledby="defModalHead" aria-hidden="true">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal"><span aria-hidden="true">&times;</span><span class="sr-only">Close</span></button>
                <h4 class="modal-title" id="defModalHead"><span class="fa fa-pencil"></span> 批量导入题目</h4>
            </div>
            <div class="modal-body block">
                <form id="user-points-form" class="form-horizontal" method="post" enctype="multipart/form-data">
                    <!-- start -->
                    <div class="form-group">
                        <label class="col-md-3 control-label">所属专业</label>

                        <div class="col-md-4 col-xs-12">
                            <input type="text" id="pop_SelMajor" class="form-control" value="" readonly />
                        </div>
                    </div>

                    <div class="form-group">
                        <label class="col-md-3 control-label">所属课程</label>

                        <div class="col-md-4 col-xs-12">
                            <input type="text" id="pop_SelCourse" class="form-control" value="" readonly />
                            <input type="hidden" id="pop_SelCourseID" value="" />
                        </div>
                    </div>

                    <div class="form-group">
                        <label class="col-md-3 control-label">选择题库数据文件（Excel）</label>

                        <div class="col-md-9 col-xs-12">
                            <input type="file" class="fileinput" name="uploadFile" id="uploadFile" title="选择上传的文件" />
                        </div>

                    </div>
                    <div class="form-group">
                        <label class="col-md-3 control-label"></label>

                        <div class="col-md-9 col-xs-12">
                            如果没有上传格式模板，请点击<a href="/Upload/Template/题库信息导入模板.xls">这里</a>下载
                        </div>

                    </div>




                    <!-- end -->
                </form>

            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-primary" onclick="return ImportExercises()">导 入</button>
            </div>

        </div>
    </div>
</div>
