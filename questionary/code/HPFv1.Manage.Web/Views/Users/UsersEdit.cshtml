                        <label class="col-md-3 control-label">昵称</label>

                        <div class="col-md-6 col-xs-12">
                            <input type="text" id="txt_nickName" class="form-control">

                        </div>

                    </div>
                    <div class="form-group">
                        <label class="col-md-3 control-label">登录邮箱</label>

                        <div class="col-md-6 col-xs-12">
                            <input id="txt_email" type="text" class="form-control" value="" />
                        </div>

                    </div>
                    
                    <div class="form-group">
                        <label class="col-md-3 control-label">登录密码</label>

                        <div class="col-md-6 col-xs-12">
                            <input id="txt_password" type="text" class="form-control" value="" />
                        </div>

                    </div>

