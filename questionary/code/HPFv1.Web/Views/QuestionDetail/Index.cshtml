@{
    Layout = null;  
}


<!DOCTYPE html>
<html lang="en">
<head>
    <!-- META SECTION -->
    <title>问卷调查</title>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1" />

    <link rel="icon" href="favicon.ico" type="image/x-icon" />
    <!-- END META SECTION -->
    <!-- CSS INCLUDE -->
    <link type="text/css" id="theme" href="~/Content/css/theme-default.css" rel="stylesheet" />
    <!-- EOF CSS INCLUDE -->
</head>
<body>
    <input type="hidden" id="QID" />
    <input type="hidden" id="GID" />
    <input type="hidden" id="Password" />

    <div id="getHtml" class="error-container">
        <div class="error-text">欢迎登录厚溥问卷调查系统</div>
        <div class="error-subtext"></div>
        <div class="error-actions">
            <div class="row">
                <div class="col-md-8">
                    <input type="password" id="txt_PassWord" placeholder="请填写调查登录密码" class="form-control" />
                </div>
                <div class="col-md-4">
                    <button class="btn btn-primary btn-block btn-default" onclick="QuestionDetailLogin()"><span>登录</span></button>
                </div>
            </div>
        </div>
    </div>
    <div id="pop_modaldialog">
    </div>
</body>
</html>



